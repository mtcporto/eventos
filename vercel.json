{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/eventos", "dest": "/api/eventos.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "functions": {
    "api/eventos.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  }
}